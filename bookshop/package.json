{
  "name": "bookshop",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "dependencies": {
    "@sap/cds": "^9",
    "@sap/cds-dk": "^9.4.3",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^2.0.4",
    "cds-plugin-ui5": "^0.13.0"
  },
  "cds": {
    "requires": {
      "db": {
        "[development]": {
          "kind": "sqlite",
          "credentials": {
            "url": "bookshop.sqlite"
          }
        }
      }
    }
  },
  "scripts": {
    "start": "cds-serve",
    "watch-bookshop": "cds watch --open com.uvs.bookshop/index.html?sap-ui-xx-viewCache=false --livereload false"
  },
  "private": true,
  "workspaces": [
    "app/*"
  ]
}
